# Example secret configuration file
# This file demonstrates the structure for secret.yaml configuration
# WARNING: Never commit actual secrets to version control!

# User passwords
passwords:
  rac: "your_rac_password_here"
  db: "your_db_password_here"
  mssql: "your_mssql_password_here"
  storeAdminPassword: "your_store_admin_password_here"
  smb: "your_smb_password_here"

# Gitea integration
gitea:
  accessToken: "your_gitea_access_token_here"

# SonarQube integration
sonarqube:
  # SonarQube authentication token
  # Generate this token in SonarQube: Administration > Security > Users > Tokens
  token: "your_sonarqube_token_here"